    <connection>report_conn</connection>
    <sql>SELECT &#xd;&#xa;  AHP_FBI.NAME_&#xd;&#xa;, AHP_FBI.workflow_entry&#xd;&#xa;, MIN&#x28;AHP_FBI.START_TIME_&#x29; AS START_&#xd;&#xa;, MAX&#x28;AHP_FBI.END_TIME_&#x29; AS END_&#xd;&#xa;, MAX&#x28;AHP_FBI.workflow_name&#x29; AS workflow_name&#xd;&#xa;, EXTRACT&#x28;DAY FROM &#x28;&#x28;MAX&#x28;AHP_FBI.END_TIME_&#x29;-MIN&#x28;AHP_FBI.START_TIME_&#x29;&#x29;&#x29;&#x29;&#x2a;24&#x2a;60&#x2a;60&#x2a;1000 AS duration&#xd;&#xa;FROM&#xd;&#xa;&#x28;SELECT&#xd;&#xa;  AHP.START_TIME_&#xd;&#xa;, AHP.END_TIME_&#xd;&#xa;, AHP.NAME_&#xd;&#xa;, SUBSTR&#x28;AHP.BUSINESS_KEY_,INSTR&#x28;AHP.BUSINESS_KEY_,&#x27;.&#x27;&#x29;&#x2b;1, INSTR&#x28;AHP.BUSINESS_KEY_,&#x27;-&#x27;&#x29;-INSTR&#x28;AHP.BUSINESS_KEY_,&#x27;.&#x27;&#x29;-1&#x29; AS workflow_name&#xd;&#xa;, SUBSTR&#x28;AHP.BUSINESS_KEY_,INSTR&#x28;AHP.BUSINESS_KEY_,&#x27;-&#x27;&#x29;&#x2b;1, 1&#x29; AS workflow_entry&#xd;&#xa;, CASE WHEN &#x28;INSTR&#x28;AHP.BUSINESS_KEY_,&#x27;.&#x27;&#x29; &#x3e; 0&#x29; AND &#x28;INSTR&#x28;AHP.BUSINESS_KEY_,&#x27;-&#x27;&#x29; &#x3e; 0&#x29; THEN &#x27;Y&#x27; ELSE &#x27;N&#x27; END  AS is_workflow_entry &#xd;&#xa;FROM ACT_HI_PROCINST AHP JOIN finished_batch_instance FBI&#xd;&#xa;ON AHP.NAME_&#x3d;FBI.identifier AND AHP.BUSINESS_KEY_ IS NOT NULL AND START_TIME_ IS NOT NULL AND END_TIME_ IS NOT NULL&#x29; AHP_FBI&#xd;&#xa;WHERE AHP_FBI.workflow_entry &#x3d; &#x27;m&#x27; and AHP_FBI.is_workflow_entry &#x3d; &#x27;Y&#x27;&#xd;&#xa;GROUP BY AHP_FBI.NAME_, AHP_FBI.workflow_entry</sql>
    <limit>0</limit>
    <lookup/>
    <execute_each_row>N</execute_each_row>
    <variables_active>N</variables_active>
    <lazy_conversion_active>N</lazy_conversion_active>
